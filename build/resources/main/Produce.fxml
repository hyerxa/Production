<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="586.0" prefWidth="613.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProduceController">

  <stylesheets>
    <URL value="@style.css" />
  </stylesheets>

  <Label layoutX="83.0" layoutY="42.0" text="Choose Product" />
  <ListView fx:id="prodList" layoutX="200.0" layoutY="42.0" prefHeight="108.0" prefWidth="275.0" />
  <Label layoutX="79.0" layoutY="183.0" text="Choose Quantity" />
  <ComboBox fx:id="quantity" layoutX="200.0" layoutY="176.0" prefHeight="30.0" prefWidth="200.0" />
  <Button fx:id="recordProduction" layoutX="249.0" layoutY="241.0" mnemonicParsing="false" onAction="#setRecordProduction" text="Record Production" />
</AnchorPane>
